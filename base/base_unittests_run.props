<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(isolateBeforeTargets)' == '' and '$(isolateAfterTargets)' == '' and '$(ConfigurationType)' != 'Makefile'">
    <isolateBeforeTargets>Midl</isolateBeforeTargets>
    <isolateAfterTargets>CustomBuild</isolateAfterTargets>
  </PropertyGroup>
  <PropertyGroup>
    <isolateDependsOn Condition="'$(ConfigurationType)' != 'Makefile'">_SelectedFiles;$(isolateDependsOn)</isolateDependsOn>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <isolate>
      <CommandLineTemplate>call &quot;$(ProjectDir)..\third_party\cygwin\setup_env.bat&quot; &amp;&amp; set CYGWIN=nontsec&amp;&amp; set OUTDIR=$(OutDir)&amp;&amp; set INPUTPATH=%(FullPath) &amp;&amp; bash -c &quot;\&quot;python\&quot; \&quot;../tools/swarming_client/isolate.py\&quot; \&quot;check\&quot; \&quot;--result\&quot; \&quot;`cygpath -m \\\&quot;${OUTDIR}\\\&quot;`/%(Filename).isolated\&quot; \&quot;--isolate\&quot; \&quot;`cygpath -m \\\&quot;${INPUTPATH}\\\&quot;`\&quot; \&quot;--path-variable\&quot; \&quot;PRODUCT_DIR\&quot; \&quot;`cygpath -m \\\&quot;${OUTDIR}\\\&quot;` \&quot; \&quot;--extra-variable\&quot; \&quot;version_full=35.0.1896.1\&quot; \&quot;--config-variable\&quot; \&quot;OS=win\&quot; \&quot;--config-variable\&quot; \&quot;chromeos=0\&quot; \&quot;--config-variable\&quot; \&quot;component=shared_library\&quot; \&quot;--config-variable\&quot; \&quot;internal_gles2_conform_tests=0\&quot; \&quot;--config-variable\&quot; \&quot;icu_use_data_file_flag=1\&quot; \&quot;--config-variable\&quot; \&quot;libpeer_target_type=static_library\&quot; \&quot;--config-variable\&quot; \&quot;use_openssl=0\&quot; \&quot;--ignore_broken_items\&quot;&quot;</CommandLineTemplate>
      <Outputs>$(OutDir)%(Filename).isolated</Outputs>
      <ExecutionDescription>isolate</ExecutionDescription>
      <AdditionalDependencies>..\tools\swarming_client\isolate.py;..\tools\swarming_client\run_isolated.py</AdditionalDependencies>
    </isolate>
  </ItemDefinitionGroup>
</Project>
